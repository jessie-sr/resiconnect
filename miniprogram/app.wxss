/**app.wxss**/
.container {
  margin: 30rpx;
  padding-top: 60rpx;
  padding-bottom: 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background: #f8f8f8;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: rgb(74, 56, 87);
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}